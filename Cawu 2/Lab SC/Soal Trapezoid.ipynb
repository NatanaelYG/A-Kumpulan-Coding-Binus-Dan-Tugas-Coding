   "source": [
    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    return np.sin(x)**2 + np.sin(x) + 2\n",
    "\n",
    "x1 = -np.pi\n",
    "x2 = 2*np.pi\n",
    "\n",
    "n = 27\n",
    "\n",
    "lebar_kotak = (x2-x1) / (n-1)\n",
    "\n",
    "x = np.linspace(x1, x2, n)\n",
    "y = f(x)\n",
    "\n",
    "trapezoid = (y[0] + 2 * sum(y[1:n-1]) + y[n-1]) * lebar_kotak / 2\n",
    "print(trapezoid)"
   ]
  }
